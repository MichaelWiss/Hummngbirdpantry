{
  "name": "hummingbirdpantry",
  "version": "0.1.0",
  "description": "Advanced pantry inventory management with AI chat, barcode scanning, and voice recognition",
  "private": true,
  "type": "module",
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@zxing/library": "^0.20.0",
    "autoprefixer": "^10.4.16",
    "clsx": "^2.1.0",
    "date-fns": "^3.0.6",
    "immer": "^10.1.3",
    "lucide-react": "^0.344.0",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.8.2",
    "react-window": "^1.8.9",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.2.2",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^1.1.0",
    "eslint": "^8.55.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^0.17.4",
    "vitest": "^1.1.0",
    "workbox-webpack-plugin": "^7.0.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:https": "vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write .",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "clean": "rm -rf dist node_modules/.vite",
    "analyze": "vite build --mode analyze",
    "secrets:scan": "detect-secrets scan > .secrets.new && detect-secrets audit .secrets.new",
    "secrets:audit": "detect-secrets audit .secrets.baseline"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "iOS >= 15"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "iOS >= 15"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/hummingbirdpantry.git"
  },
  "keywords": [
    "pantry",
    "inventory",
    "react",
    "typescript",
    "pwa",
    "voice-recognition",
    "barcode-scanning",
    "meal-planning"
  ],
  "author": "Your Name",
  "license": "MIT"
}
